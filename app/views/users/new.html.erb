<h1>Sign up</h1>
<%= form_for(@user) do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
	<%= render 'fields', :f => f %>
	<!-- in above line, :f is the f variable in the partial file: _fields.hteml.erb -->
	<!--  and f is the passing value from this file-->
	<div class="actions">
		<%= f.submit "Sign up" %>
	</div>
<% end %>